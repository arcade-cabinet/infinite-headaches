{
  "name": "homestead-headaches",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "install-and-dev": "pnpm install && pnpm dev",
    "build": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:watch": "vite build --watch --mode development",
    "preview": "vite preview --port 8080",
    "tsc": "tsc",
    "tscgo": "tsgo",
    "tsc-watch": "tsc --watch",
    "dev:prod": "concurrently \"npm run build:watch\" \"npm run preview\"",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "format": "biome format . --write",
    "check": "biome check . && tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "electron:dev": "cd electron && npm run electron:start-live",
    "electron:build": "cd electron && npm run electron:build",
    "electron:pack": "cd electron && npm run electron:pack",
    "electron:make": "cd electron && npm run electron:make",
    "build:capacitor": "CAPACITOR_BUILD=true vite build --mode production",
    "native:sync": "pnpm build:capacitor && npx cap sync",
    "native:android": "pnpm native:sync && npx cap open android",
    "native:ios": "pnpm native:sync && npx cap open ios",
    "native:electron": "pnpm native:sync && pnpm electron:dev"
  },
  "dependencies": {
    "@babylonjs/core": "^8.49.6",
    "@babylonjs/loaders": "^8.49.6",
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.2",
    "@capacitor/device": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/screen-orientation": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "animejs": "^4.3.5",
    "clsx": "^2.1.1",
    "framer-motion": "12.9.2",
    "lucide-react": "^0.503.0",
    "miniplex": "^2.0.0",
    "miniplex-react": "^2.0.1",
    "react": "^19.2.4",
    "react-babylonjs": "3.2.5-beta.2",
    "react-dom": "^19.2.4",
    "seedrandom": "^3.0.5",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "yuka": "^0.7.8",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@capacitor/cli": "^8.0.2",
    "@google/genai": "^1.39.0",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/animejs": "^3.1.13",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/seedrandom": "^3.0.8",
    "@types/yuka": "^0.7.4",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "babylonjs": "^8.49.6",
    "babylonjs-loaders": "^8.49.6",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "happy-dom": "^20.4.0",
    "playwright": "^1.58.1",
    "puppeteer": "^24.36.1",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^4.0.18"
  }
}
