{
  "name": "infinite-headaches",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "install-and-dev": "pnpm install && pnpm dev",
    "build": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:watch": "vite build --watch --mode development",
    "preview": "vite preview --port 8080",
    "tsc": "tsc",
    "tscgo": "tsgo",
    "tsc-watch": "tsc --watch",
    "dev:prod": "concurrently \"npm run build:watch\" \"npm run preview\"",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "format": "biome format . --write",
    "check": "biome check . && tsgo --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "electron:dev": "cd electron && npm run electron:start-live",
    "electron:build": "cd electron && npm run electron:build",
    "electron:pack": "cd electron && npm run electron:pack",
    "electron:make": "cd electron && npm run electron:make",
    "native:sync": "pnpm build:prod && npx cap sync",
    "native:android": "pnpm native:sync && npx cap open android",
    "native:ios": "pnpm native:sync && npx cap open ios",
    "native:electron": "pnpm native:sync && pnpm electron:dev",
    "audio:capture": "echo 'Open browser dev tools and run: window.captureAudio.captureAllAudio()'"
  },
